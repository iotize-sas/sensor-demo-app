    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android": "ionic cordova run android",
    "ios": "ionic cordova run ios",
    "webapp": "npm run webapp:private",
    "webapp:private": "ng serve",
    "webapp:public": "ng serve --host 0.0.0.0 --ssl true",
    "deploy:firebase": "ng build --prod && firebase deploy",
    "deploy:playstore": "bundle exec fastlane android deploy",
    "deploy:playstore:beta": "bundle exec fastlane android beta",
    "deploy:playstore:debug": "bundle exec fastlane android debug",
    "android:release": "ionic cordova build android --prod --release -- -- --keystore=$CORDOVA_ANDROID_KEYSTORE_PATH --storePassword=$CORDOVA_ANDROID_KEYSTORE_PASSWORD --alias=$CORDOVA_ANDROID_KEYSTORE_ALIAS --password=$CORDOVA_ANDROID_KEY_PASSWORD"
